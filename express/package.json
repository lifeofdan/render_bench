    "@types/node": "^18.0.3",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
